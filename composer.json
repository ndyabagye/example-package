{
    "name": "laravelnews/feeds",
    "description": "Get articles from Laravel-News.com",
    "type": "library",
    "autoload": {
        "psr-4": {
            "LaravelNews\\Feed\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.5",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
